<?xml version="1.0" encoding="utf-8"?>
<resources>

<!--   Style for rounded corners -->
    <style name="RoundedCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">8dp</item>
    </style>
    <style name="FullRoundedCorners">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="ThemeOverlay.SearchView" parent="">
    <!-- Text color -->
    <item name="android:editTextColor">@color/white</item>
    <!-- Hint text color -->
    <item name="android:textColorHint">@color/transhparent_gray</item>
        <item name="colorControlActivated">@color/gray</item>
    </style>
    <style name="BottomNavigationActiveIndicator" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@android:color/transparent</item>
    </style>

</resources>